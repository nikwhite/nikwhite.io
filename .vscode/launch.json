{
  "configurations": [
    {
      "name": "Cloud Run: Run/Debug Locally",
      "type": "cloudcode.cloudrun",
      "request": "launch",
      "build": {
        "docker": {
          "path": "services/signal/Dockerfile"
        }
      },
      "image": "nikwhite.io/signal",
      "service": {
        "name": "signal",
        "containerPort": 8080,
        "resources": {
          "limits": {
            "cpu": 1,
            "memory": "256Mi"
          }
        }
      },
      "target": {
        "minikube": {}
      },
      "watch": true,
      "externalPortForward": 8080
    }
  ]
}